<?xml version="1.0" encoding="UTF-8"?>
<model name="devices" version="1.0.0">
    <depends>
        <bundle name="legooCore" version="1.0.0" use="required"/>
        <bundle name="entities" version="1.0.0" use="optional"/>
    </depends>
    <evevts>
        <listen channel="device-command-channel">
            <event name="loadDeviceByImeiEvent"/>
            <event name="loadDeviceDtoByIdEvent"/>
            <event name="saveOrUpdateDeviceEvent"/>
            <event name="changeDeviceToNormalEvent"/>
            <event name="changeDeviceToRepairEvent"/>
            <event name="changeDeviceToScrapEvent"/>
        </listen>
    </evevts>
</model>